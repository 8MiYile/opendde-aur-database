pkgver() {
	    cd "$srcdir/$_pkgname"
	        echo "$(git log -1 --format="%cd" --date=short | sed 's|-||g').$(git rev-list --count master)"
	}

	package() {
		    cd "$srcdir/$_pkgname"
		        install -Dm755 xdg-open "$pkgdir/usr/bin/xdg-open"
			    install -Dm644 config "$pkgdir/etc/xdg/busking/config"
		    }
