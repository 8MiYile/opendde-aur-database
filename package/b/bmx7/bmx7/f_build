build() {
  cd "$srcdir/$pkgname/src"
  make
  #JSON plugin
  make -C lib/bmx7_json/
  #SMS plugin
  make -C lib/bmx7_sms/
  #Table plugin
#  make -C lib/bmx7_table/
  #topology plugin
  make -C lib/bmx7_topology/
  #tun plugin
  make -C lib/bmx7_tun/
  #evil plugin
  make -C lib/bmx7_evil/
  #http-info plugin
  make -C lib/bmx7_http_info/
}
