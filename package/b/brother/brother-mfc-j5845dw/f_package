        install -m 755 -D "${pkgdir}/opt/brother/Printers/mfcj5845dw/cupswrapper/brother_lpdwrapper_mfcj5845dw" "${pkgdir}/usr/lib/cups/filter/brother_lpdwrapper_mfcj5845dw"
}
