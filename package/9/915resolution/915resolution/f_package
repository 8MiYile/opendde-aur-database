package() {
  cd "${srcdir}/${pkgname}-${pkgver}"

  # Install the program.
  install -Dm755 915resolution "${pkgdir}/usr/bin/915resolution"
  install -Dm755 dump_bios "${pkgdir}/usr/bin/915dump_bios"

  # Install the license.
  install -Dm755 LICENSE.txt "${pkgdir}/usr/share/licenses/${pkgname}/LICENSE"
}
