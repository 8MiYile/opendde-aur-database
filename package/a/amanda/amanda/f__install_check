_install_check() {
  local _ckvar
  local _ckline
  for _ckvar in _amlibexec _amsecurity _amhome; do
    _ckline="${_ckvar}='${!_ckvar}'"
    if ! grep -q "^${_ckline}"'$' "${startdir}/${install}"; then
      set +u
      msg "${install} must be fixed"
      echo "${_ckline}"
      false
    fi
  done
}
