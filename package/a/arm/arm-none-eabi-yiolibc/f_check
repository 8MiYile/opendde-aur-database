check() {
	${_prefix}gcc --print-multi-lib |
	head -n1 |
	while IFS=';' read dir opts; do
		(
			tmp=$(mktemp)
			trap 'rm "$tmp"' EXIT
			set -x
			${_prefix}gcc --specs=rdimon.specs -L"build/$dir/src" -I"build/$dir/gen" $opts -xc - -lyio -o "$tmp" <<'EOF'
#include <yio.h>
int main() {
	yprint("Hello world", 5, "\n");
}
