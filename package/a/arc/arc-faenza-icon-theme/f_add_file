add_file() {
	source_dir="$1"
	i="$2"
	filename=${i%.png}
	png="${filename}.png"
	svg="${filename}.svg"
	rm -f "$svg" "$png"
	cp ${source_dir}/${png} .
	if [ "$size" == "22" ]; then
		rm -f "../../panel/${size}/$svg" "../../panel/${size}/$png"
		ln -s "../../status/${size}/$png" "../../panel/${size}/$png"
	fi
}
